{
  "name": "phu1237/laravel-awesome-validation",
  "description": "Collection of useful Traits, etc for Laravel Validation",
  "license": "MIT",
  "authors": [
    {
      "name": "Phu1237",
      "email": "phu1237@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "pestphp/pest": "^3.7",
    "orchestra/testbench": "^9.9",
    "pestphp/pest-plugin-laravel": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Phu1237\\AwesomeValidation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Phu1237\\AwesomeValidation\\ServiceProvider"
      ]
    }
  }
}
